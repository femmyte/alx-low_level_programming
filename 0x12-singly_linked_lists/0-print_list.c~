#include "lists.h"
#include <stdio.h>

/**
 * print_list - singly linked list
 * @h: string - (malloc'ed string)
 * Return: Always len
 * 
*/

size_t print_list(const list_t *h)
{
	int len = 0;
	const list_t *temp = h;

	while (temp != NULL)
	{
		printf("[%d] %s\n", temp->len, temp->str != NULL ? temp->str : "(nil)");
		temp = temp->next;
		len++;
	}
	return (len);
	
}

